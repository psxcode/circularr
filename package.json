{
  "name": "circularr",
  "version": "0.1.3",
  "description": "Circular fixed size array",
  "keywords": [
    "circular",
    "fixed",
    "array"
  ],
  "main": "build/index",
  "module": "build/index",
  "typings": "build/index.d.ts",
  "files": [
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "psxcode <psxcode@gmail.com> (https://github.com/psxcode)",
  "homepage": "https://github.com/psxcode/circularr",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "NODE_ENV=test nyc mocha 'test/*.spec.ts'",
    "build": "rimraf build && yarn build:js && yarn build:dts",
    "copy": "cp ./{package.json,readme.md,LICENSE} ./build",
    "build:dts": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:js": "NODE_ENV=production babel src --out-dir build --extensions '.ts,.js' --source-maps 'inline'"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.24",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.2.0",
    "eslint": "^5.11.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-typescript": "^1.0.0-0",
    "esm": "^3.2.4",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "require": [
      "./tshook.js",
      "esm"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
